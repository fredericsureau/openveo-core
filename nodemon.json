{
  "restartable": "rs",
  "ignore": [
    ".git",
    ".sass-cache",
    "node_modules/**/node_modules",
    "node_modules/**/public",
    "app/client",
    "config",
    "tasks"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "watch": [
    "."
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "ext": "js json"
}